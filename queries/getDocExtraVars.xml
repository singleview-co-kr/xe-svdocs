    <tables>
        <table name="svdocs_extra_vars" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" default="-1" notnull="notnull" pipe="and" />
        <condition operation="equal" column="doc_srl" var="doc_srl" notnull="notnull" pipe="and" />
    </conditions>
</query>